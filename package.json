{
	"name": "@capitec/omni-router",
	"version": "1.0.3",
	"description": "Simple path-based web component router",
	"author": "Capitec",
	"license": "MIT",
	"homepage": "https://github.com/capitec/omni-router",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/capitec/omni-router.git"
	},
	"bugs": {
		"url": "https://github.com/capitec/omni-router/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": "16.x"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"test": "echo \"Error: no test specified\" && exit 0",
		"lint": "eslint src/**/*.{js,ts}",
		"docs": "npm run docs:api && npm run docs:contributors",
		"docs:api": "npx typedoc --out docs src/index.ts",
		"docs:contributors": "npx all-contributors generate",
		"contributors:add": "npx all-contributors add",
		"prepublish": "npm run build"
	},
	"keywords": [
		"web components",
		"router"
	],
	"dependencies": {
		"query-string": "^8.0.3",
		"regexparam": "^2.0.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"all-contributors-cli": "^6.20.0",
		"eslint": "^8.15.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"typedoc": "^0.22.15",
		"typescript": "^4.6.4"
	}
}
